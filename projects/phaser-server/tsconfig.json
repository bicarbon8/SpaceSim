{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist",
    "sourceMap": true,
    "declaration": true,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "noEmit": false,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES6",
    "module": "ES6",
    "lib": [
      "es2018",
      "dom",
      "scripthost"
    ],
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": false,
  }
}