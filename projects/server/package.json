{
  "name": "space-sim-server",
  "version": "1.0.0",
  "description": "game server for multiplayer version of Spaceship Game",
  "type": "module",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "webpack",
    "build:dev": "npm run clean && npm run build",
    "start": "npm run build && npm run open",
    "start:dev": "npm run build && npm run open:dev",
    "open": "npx phaser-game-server -s -c server.config.prod.json",
    "open:dev": "npx phaser-game-server -s",
    "test": ""
  },
  "keywords": [
    "phaser",
    "html game",
    "typescript game",
    "spaceship game"
  ],
  "author": "Jason Holt Smith <bicarbon8@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "canvas": "^2.10.2",
    "datauri": "^4.1.0",
    "dynamic-data-store": "^2.0.1",
    "express": "^4.18.2",
    "jsdom": "^20.0.3",
    "phaser": "^3.55.2",
    "phaser-game-server": "^1.1.0",
    "phaser-game-server-engine": "^1.1.0",
    "socket.io": "^4.5.4",
    "space-sim-shared": "^1.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jsdom": "^20.0.1",
    "copy-webpack-plugin": "^11.0.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
